{
	"name": "node-barebone",
	"version": "1.0.0",
	"main": "index.js",
	"author": "Rutvij Kanzaria",
	"license": "MIT",
	"scripts": {
		"start": "yarn dev:start",
		"dev:start": "nodemon -e js --ignore lib --ignore dist --exec babel-node src/server",
		"dev:wds": "webpack-dev-server --progress",
		"prod:build": "rimraf lib dist && babel src -d lib --presets=es2015,react,stage-2 --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
		"prod:start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
		"prod:stop": "pm2 delete server",
		"test": "eslint src",
		"fix": "eslint src --fix",
		"prepush": "yarn test",
		"precommit": "yarn test"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.5",
		"babel-loader": "^7.1.4",
		"babel-plugin-import": "^1.8.0",
		"babel-plugin-transform-decorators-legacy": "^1.3.5",
		"babel-preset-env": "^1.7.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"cross-env": "^5.2.0",
		"css-loader": "^0.28.11",
		"eslint": "^4.10.0",
		"eslint-config-airbnb": "^17.0.0",
		"eslint-import-resolver-webpack": "^0.10.1",
		"eslint-plugin-compat": "^2.4.0",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.9.1",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^0.14.3",
		"nodemon": "^1.17.5",
		"pm2": "^2.10.4",
		"redux-devtools-extension": "^2.13.5",
		"rimraf": "^2.6.2",
		"style-loader": "^0.21.0",
		"webpack": "^4.12.1",
		"webpack-cli": "^3.0.8",
		"webpack-dev-server": "^3.1.4"
	},
	"browserslist": [
		"> 1%"
	],
	"dependencies": {
		"antd": "^3.8.0",
		"axios": "^0.18.0",
		"babel-polyfill": "^6.26.0",
		"body-parser": "^1.18.3",
		"compression": "^1.7.2",
		"connected-react-router": "^4.3.0",
		"express": "^4.16.3",
		"history": "^4.7.2",
		"immutable": "^3.8.2",
		"lodash": "^4.17.10",
		"mongoose": "^5.1.6",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"react-hot-loader": "^4.3.3",
		"react-icons": "^2.2.7",
		"react-loadable": "^5.5.0",
		"react-prop-types": "^0.4.0",
		"react-redux": "^5.0.7",
		"react-router": "^4.3.1",
		"react-router-dom": "^4.3.1",
		"redux": "^4.0.0",
		"redux-immutable": "^4.0.0",
		"redux-logger": "^3.0.6",
		"redux-saga": "^0.16.0",
